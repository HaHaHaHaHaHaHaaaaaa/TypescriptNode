{
  "name": "typescriptexpress",
  "version": "1.0.0",
  "description": "NodeGamesWebsite with Typescript",
  "main": "index.ts",
  "scripts": {
    "start:dev": "npm run build:dev && node dist/main.js",
    "start:prod": "npm run build:prod && node dist/main.js",
    "build:prod": "webpack --config webpack.config.ts --progress --color && mv ./build/** dist/src/public/static && rm -r ./build",
    "build:dev": " webpack --config webpack.dev.config.ts --progress --color  && mv ./src/public/build/** dist/src/public/static && rm -r ./src/public/build",
    "tslint": "tslint -c tslint.json './**/*.ts'"
  },
